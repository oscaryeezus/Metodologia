Proceso incrementoSi
	Escribir "Algoritmo DOBLAR NUMERO Y MULTIPLICARLO POR 25";
	Escribir "Autor: Oscar Daniel Morales Navarro";
	
	Definir num Como Real;
	
	Escribir "Ingrese un numero";Leer num;
	si num > 0 Entonces
		Escribir "EL resultado de la operacion es ",num*2*25;
	FinSi
	si num <= 0 Entonces
		Escribir "Numero invalido";
	FinSi
	
FinAlgoritmo
